{
	"permissions": "*",
	"routing": "routing.json",
	"autoload": {
		"directories": ["libraries"],
		"files": [
			{
				"file": "libraries/io/io.php",
				"function": true
			},
			{
				"file": "libraries/utility/utility.php",
				"function": true
			}
		]
	},
	"dependencies": ["PhpParser"],
	"languages":{
		"fa_IR": "langs/fa_IR.json",
		"en_US": "langs/en_US.json"
	},
	"storages": {
		"logs": {
			"@class": "local",
			"type": "protected",
			"root": "storage/protected/logs"
		},
		"http_client_cookies": {
			"@class": "local",
			"type": "private",
			"root": "storage/private/http_client_cookies"
		}
	}
}
